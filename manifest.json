{
    "name": "Test Case Adm",
    "description": "Help you determine which test cases you need & additional Information",
    "version": "1.0",
    "manifest_version": 3,
    "background": {
        "service_worker": "background.js"
    },
    "permissions": ["storage"],

    "action": {
        "default_popup": "popup.html",
        "default_icon": {
            "16": "icon16.png",
            "32": "icon32.png"
        }
    },
    "icons": {
        "16": "icon16.png",
        "32": "icon32.png"
    }




}